<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#406BC5D6">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:angle="135"
                android:startColor="#306BC5D6"
                android:endColor="#156BC5D6" />
            <corners android:radius="20dp" />
            <stroke
                android:width="2dp"
                android:color="#806BC5D6" />
        </shape>
    </item>
</ripple>

